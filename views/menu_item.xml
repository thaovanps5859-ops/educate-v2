<odoo>

    <data>
        <record id="action_backend_dashboard" model="ir.actions.client">
                <field name="name">Dashboard</field>
                <field name="tag">action_school_dashboard</field>
                <field name="path">school-dashboard</field>
        </record>

        <record id="action_student_transcript" model="ir.actions.client">
                <field name="name">Transcript</field>
                <field name="tag">action_dashboard_transcript</field>
                <field name="path">student-transcript</field>
        </record>

        <record id="action_student_gradesheet" model="ir.actions.client">
                <field name="name">Gradesheet</field>
                <field name="tag">action_dashboard_gradesheet</field>
                <field name="path">student-gradesheet</field>
        </record>

        <record id="action_partner_form_inherit" model="ir.actions.act_window">
            <field name="name">Parents</field>
            <field name="res_model">res.partner</field>
            <field name="view_mode">kanban,list,form</field>
            <field name="domain">[('is_parent', '=', True)]</field>
            <field name="search_view_id" ref="base.view_res_partner_filter"/>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create a Parent Contact in your address book
              </p><p>
                Odoo helps you track all activities related to your parents contact.
              </p>
            </field>
        </record>

        <record id="product_normal_action_fee_element" model="ir.actions.act_window">
            <field name="name">Fee Elements</field>
            <field name="res_model">product.product</field>
            <field name="view_mode">kanban,list,form,activity</field>
            <field name="context">{"search_default_fee_element":1}</field>
            <field name="path">school-fee-elements</field>
            <field name="view_id" ref="product.product_product_tree_view"/>
            <field name="search_view_id" ref="product_product_search_inherit_fee_element"/>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create a new fee element
              </p>
            </field>
        </record>
    
